<main class="c-layout c-layout-profile">
  <div class="c-layout-profile__back">
    {{> Link to=urlBack className="c-router-arrow" }}
  </div>
  <div class="c-layout-profile__wrap">
    <div class="c-layout-profile__avatar">
      <div class="c-profile-avatar">
        <span class="c-profile-avatar__upload" onclick="console.log('upload')">
          Поменять аватар
        </span>
        <img class="c-profile-avatar__img" src="/icons/avatar-default.jpg" alt="Avatar">
      </div>
      {{> Title text=title className="c-layout-profile__title" }}
    </div>
    <div class="c-layout-profile__body">
      {{> @partial-block }}
    </div>
  </div>
</main>
